{
  "name": "landsat-web-tracker",
  "private": true,
  "scripts": {
    "frontend:dev": "pnpm --prefix ./frontend/ dev",
    "frontend:lint": "pnpm --prefix ./frontend/ lint",
    "frontend:format": "pnpm --prefix ./frontend/ format",
    "backend:dev": "poetry run uvicorn backend.main:app --port 8000 --reload --reload-dir ./backend",
    "backend:openapi": "poetry run openapi",
    "backend:lint": "poetry run ruff check",
    "backend:format": "poetry run ruff check --fix",
    "backend:test": "poetry run pytest ./backend.test/",
    "dev": "pnpm run /:dev$/",
    "lint": "pnpm run /:lint$/",
    "format": "pnpm run /:format$/",
    "test": "pnpm run /:test$/",
    "postinstall": "poetry install && poetry run openapi",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.6"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c"
}
